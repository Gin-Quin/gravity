{
  "name": "doc-svelte",
  "version": "0.0.1",
  "scripts": {
    "dev": "esrun scripts/generate-documentation-routes && chokidar 'src/documentation/**/*' -c 'esrun scripts/generate-documentation-routes' & svelte-kit dev",
    "build": "svelte-kit build",
    "generate:documentation": "esrun scripts/generate-documentation-routes",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@digitak/esrun": "^3.1.2",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-static": "^1.0.0-next.28",
    "@sveltejs/kit": "next",
    "@types/js-yaml": "^4.0.5",
    "chokidar-cli": "^3.0.0",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.0.1",
    "js-yaml": "^4.1.0",
    "mdsvex": "^0.10.5",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-material-icons": "^2.0.0",
    "svelte-preprocess": "^4.10.1",
    "tslib": "^2.3.1",
    "typescript": "~4.5.4"
  },
  "type": "module",
  "dependencies": {
    "sass": "^1.49.9"
  }
}